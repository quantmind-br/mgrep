{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "module": "NodeNext",
    "target": "ES2022",
    "rootDir": "src",
    "incremental": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext"
  }
}
